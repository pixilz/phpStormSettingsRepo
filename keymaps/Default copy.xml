<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CloseActiveTab" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="ConsoleView.ClearAll">
    <keyboard-shortcut first-keystroke="shift ctrl alt semicolon" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="EditorCodeBlockEnd" />
  <action id="EditorDeleteLine" />
  <action id="EditorDuplicate" />
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorSelectWord" />
  <action id="ExpandAll">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="ExpandExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="ctrl close_bracket" />
  </action>
  <action id="GotoTest" />
  <action id="Images.Editor.ZoomIn">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="Images.ShowThumbnails" />
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="RunDashboard.ShowConfigurations" />
  <action id="ScrollFromSource">
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt s" />
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="UML.ZoomIn">
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
</keymap>